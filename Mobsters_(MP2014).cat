                            <infoLink id="62b9-d49b-91db-c01e" name="Explosive &quot;X&quot;" hidden="false" targetId="732f-8019-c442-218b" type="rule"/>
                            <infoLink id="72c4-5013-7593-06db" name="Fire" hidden="false" targetId="18fd-a5af-bc8e-ffd1" type="rule"/>
