                  <constraints>
                    <constraint field="selections" scope="parent" value="1.0" percentValue="false" shared="true" includeChildSelections="false" includeChildForces="false" id="8f83-1209-7120-c369" type="max"/>
                  </constraints>
                  <profiles>
                    <profile id="ab69-99c2-e784-6fdd" name="4&gt; Swerve" hidden="false" typeId="2ea0-3862-98c1-ff45" typeName="&lt;5 Judge Talent">
                      <characteristics>
                        <characteristic name="Skill Tree" typeId="6801-e6c5-beec-2990">Lawmaster Mastery</characteristic>
                        <characteristic name="Branch No." typeId="a643-c188-7f61-6530">4</characteristic>
