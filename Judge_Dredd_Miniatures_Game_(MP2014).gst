                      </constraints>
                      <profiles>
                        <profile id="075f-35ba-628c-f12e" name="1.0&gt; Martial Artist" hidden="false" typeId="dce6-cce1-f770-9296" typeName="&lt;6 Weapon Talent">
                          <characteristics>
