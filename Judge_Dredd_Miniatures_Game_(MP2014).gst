                    <profile id="1691-df86-1cdd-c12a" name="1&gt; Leg Shot" hidden="false" typeId="2ea0-3862-98c1-ff45" typeName="&lt;5 Judge Talent">
                      <characteristics>
                        <characteristic name="Skill Tree" typeId="6801-e6c5-beec-2990">Shoot to Wound</characteristic>
