                        <condition field="selections" scope="ancestor" value="0.0" percentValue="false" shared="true" includeChildSelections="false" includeChildForces="false" childId="1c7f-4ed2-32e0-cb44" type="instanceOf"/>
                      </conditions>
                    </conditionGroup>
