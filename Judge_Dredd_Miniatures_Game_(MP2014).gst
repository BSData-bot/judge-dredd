                  </constraints>
                  <profiles>
                    <profile id="b8f1-4c7b-827c-4429" name="3.0&gt; Mental Sting" hidden="false" typeId="e687-4aea-3b8d-c5c5" typeName="&lt;8 Psi Talent">
