                    <selectionEntryGroup id="d9f0-9c5e-f33f-c2e9" name="[Twin Shooter]" hidden="false" collective="false" import="true">
                      <selectionEntries>
                        <selectionEntry id="2055-fd3c-96d6-17a4" name="1&gt; Dual Shooter" hidden="false" collective="false" import="true" type="upgrade">
