                      </constraints>
                      <profiles>
                        <profile id="3b06-eea3-f427-666f" name="1.1A&gt; Rubber Takedown" hidden="false" typeId="2ea0-3862-98c1-ff45" typeName="&lt;5 Judge Talent">
