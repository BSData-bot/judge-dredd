              <selectionEntries>
                <selectionEntry id="d55f-4f61-50eb-8346" name="1.0&gt; Skilled and Deadly" hidden="false" collective="false" import="true" type="upgrade">
                  <constraints>
                    <constraint field="selections" scope="parent" value="1.0" percentValue="false" shared="true" includeChildSelections="false" includeChildForces="false" id="0a9c-fc0a-a687-4744" type="max"/>
                  </constraints>
                  <profiles>
                    <profile id="0fb5-f2b6-81b8-f7db" name="1.0&gt; Skilled and Deadly" hidden="false" typeId="dce6-cce1-f770-9296" typeName="&lt;6 Weapon Talent">
