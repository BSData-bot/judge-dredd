                      </constraints>
                      <profiles>
                        <profile id="58c1-6370-b9a6-0363" name="1&gt; Frag out!" hidden="false" typeId="dce6-cce1-f770-9296" typeName="&lt;6 Weapon Talent">
                          <characteristics>
                            <characteristic name="Skill Tree" typeId="cfb6-ab49-c78a-1c8d">Demolition Man</characteristic>
