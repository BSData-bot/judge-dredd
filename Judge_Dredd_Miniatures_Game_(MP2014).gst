                      </constraints>
                      <profiles>
                        <profile id="f253-15e1-0c95-cdae" name="2&gt; Incinerating Finger" hidden="false" typeId="e687-4aea-3b8d-c5c5" typeName="&lt;8 Psi Talent">
                          <characteristics>
                            <characteristic name="Skill Tree" typeId="6a59-14eb-98de-75b6">Pyrokine</characteristic>
