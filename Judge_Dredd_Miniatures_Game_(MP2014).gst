                        <constraint field="selections" scope="parent" value="1.0" percentValue="false" shared="true" includeChildSelections="false" includeChildForces="false" id="09b0-ccf8-7719-72c6" type="max"/>
                      </constraints>
                      <profiles>
                        <profile id="1937-6de4-2b56-1c6c" name="2.1&gt; Smackdown" hidden="false" typeId="dce6-cce1-f770-9296" typeName="&lt;6 Weapon Talent">
