                            <cost name=" Credits" typeId="Credits" value="0.0"/>
                          </costs>
                        </selectionEntry>
                        <selectionEntry id="776a-cf66-93df-14f1" name="2&gt; Long Lob" hidden="false" collective="false" import="true" type="upgrade">
